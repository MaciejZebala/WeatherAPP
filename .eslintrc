{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "env": {
        "browser": true,
        "node": true
    },
  "rules": {
      "prettier/prettier": ["error", {
        "endOfLine":"auto"
      }],
      "no-plusplus": "off",
      "no-alert": "off",
      "camelcase": ["error",{ "ignoreDestructuring": true }],
      "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
